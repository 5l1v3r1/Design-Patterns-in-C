{
"add_file_header":"True",
"namespace":      "decorator",
"path":           "decorator",
"trace":          "True",

"classes": {
	"interface":{
		"type":"interface",
		"members": [
			["pure_virtual", "write", "void", "char *buf"],
			["pure_virtual", "read", "void", "char *buf"]
		],

		"inheritance": {
			"core":{
				"members": [ ["override",  "<ALL>"] ]
			},
			"decorator":{
				"enable_destructor":"True",
				"members": [ ["override",  "<ALL>"],
					["method","decorator", "void", "struct interface *inner"],
					["var",   "_inner",  "struct interface *", "", "private"]
				],

				"inheritance": {
					"wrapper":{
						"members": [ ["override",  "<ALL>"],
							["method","wrapper", "void", "struct interface *inner, char *str"]
						]
					}
				}
			}
		}
	},

	"test":{
		"includes":["wrapper", "core"],
		"templates":["_test"]
	}
}
}
